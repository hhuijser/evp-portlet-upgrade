<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder auto-namespace-tables="false" package-path="com.liferay.calendar">
	<author>Eduardo Lundgren</author>
	<namespace>Calendar</namespace>
	<entity local-service="true" name="Calendar" remote-service="true" uuid="true">
		<!-- PK fields -->
		<column name="calendarId" primary="true" type="long"></column>
		<!-- Group instance -->
		<column name="groupId" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<!-- Permission fields -->
		<column name="resourceBlockId" type="long"></column>
		<!-- Other fields -->
		<column name="calendarResourceId" type="long"></column>
		<column localized="true" name="name" type="String"></column>
		<column localized="true" name="description" type="String"></column>
		<column name="timeZoneId" type="String"></column>
		<column name="color" type="int"></column>
		<column name="defaultCalendar" type="boolean"></column>
		<column name="enableComments" type="boolean"></column>
		<column name="enableRatings" type="boolean"></column>
		<!-- Order -->
		<order by="asc">
			<order-column name="name"></order-column>
		</order>
		<!-- Finder methods -->
		<finder name="G_C" return-type="Collection">
			<finder-column name="groupId"></finder-column>
			<finder-column name="calendarResourceId"></finder-column>
		</finder>
		<finder name="G_C_D" return-type="Collection">
			<finder-column name="groupId"></finder-column>
			<finder-column name="calendarResourceId"></finder-column>
			<finder-column name="defaultCalendar"></finder-column>
		</finder>
	</entity>
	<entity local-service="true" name="CalendarBooking" remote-service="true" trash-enabled="true" uuid="true">
		<!-- PK fields -->
		<column name="calendarBookingId" primary="true" type="long"></column>
		<!-- Group instance -->
		<column name="groupId" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<!-- Permission fields -->
		<column name="resourceBlockId" type="long"></column>
		<!-- Other fields -->
		<column name="calendarId" type="long"></column>
		<column name="calendarResourceId" type="long"></column>
		<column name="parentCalendarBookingId" type="long"></column>
		<column name="vEventUid" type="String"></column>
		<column localized="true" name="title" type="String"></column>
		<column localized="true" name="description" type="String"></column>
		<column name="location" type="String"></column>
		<column name="startTime" type="long"></column>
		<column name="endTime" type="long"></column>
		<column name="allDay" type="boolean"></column>
		<column name="recurrence" type="String"></column>
		<column name="firstReminder" type="long"></column>
		<column name="firstReminderType" type="String"></column>
		<column name="secondReminder" type="long"></column>
		<column name="secondReminderType" type="String"></column>
		<column name="status" type="int"></column>
		<column name="statusByUserId" type="long"></column>
		<column name="statusByUserName" type="String"></column>
		<column name="statusDate" type="Date"></column>
		<!-- Order -->
		<order by="asc">
			<order-column name="startTime"></order-column>
			<order-column case-sensitive="false" name="title"></order-column>
		</order>
		<!-- Finder methods -->
		<finder name="CalendarId" return-type="Collection">
			<finder-column name="calendarId"></finder-column>
		</finder>
		<finder name="CalendarResourceId" return-type="Collection">
			<finder-column name="calendarResourceId"></finder-column>
		</finder>
		<finder name="ParentCalendarBookingId" return-type="Collection">
			<finder-column name="parentCalendarBookingId"></finder-column>
		</finder>
		<finder name="C_P" return-type="CalendarBooking" unique="true">
			<finder-column name="calendarId"></finder-column>
			<finder-column name="parentCalendarBookingId"></finder-column>
		</finder>
		<finder name="C_V" return-type="CalendarBooking" unique="true">
			<finder-column name="calendarId"></finder-column>
			<finder-column name="vEventUid"></finder-column>
		</finder>
		<finder name="C_S" return-type="Collection">
			<finder-column name="calendarId"></finder-column>
			<finder-column arrayable-operator="OR" name="status"></finder-column>
		</finder>
		<finder name="P_S" return-type="Collection">
			<finder-column name="parentCalendarBookingId"></finder-column>
			<finder-column name="status"></finder-column>
		</finder>
		<!-- References -->
		<reference entity="Mail" package-path="com.liferay.mail"></reference>
		<reference entity="Company" package-path="com.liferay.portal"></reference>
		<reference entity="Group" package-path="com.liferay.portal"></reference>
		<reference entity="Subscription" package-path="com.liferay.portal"></reference>
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="AssetLink" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="MBMessage" package-path="com.liferay.portlet.messageboards"></reference>
		<reference entity="RatingsStats" package-path="com.liferay.portlet.ratings"></reference>
		<reference entity="SocialActivity" package-path="com.liferay.portlet.social"></reference>
		<reference entity="SocialActivityCounter" package-path="com.liferay.portlet.social"></reference>
		<reference entity="TrashEntry" package-path="com.liferay.portlet.trash"></reference>
	</entity>
	<entity local-service="true" name="CalendarImporter" remote-service="false">
		<!-- References -->
		<reference entity="ClassName" package-path="com.liferay.portal"></reference>
		<reference entity="Group" package-path="com.liferay.portal"></reference>
		<reference entity="ResourceAction" package-path="com.liferay.portal"></reference>
		<reference entity="ResourceBlock" package-path="com.liferay.portal"></reference>
		<reference entity="ResourcePermission" package-path="com.liferay.portal"></reference>
		<reference entity="Subscription" package-path="com.liferay.portal"></reference>
		<reference entity="AssetCategory" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="AssetLink" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="AssetVocabulary" package-path="com.liferay.portlet.asset"></reference>
		<reference entity="CalEvent" package-path="com.liferay.portlet.calendar"></reference>
		<reference entity="CalEvent" package-path="com.liferay.portlet.calendar"></reference>
		<reference entity="MBDiscussion" package-path="com.liferay.portlet.messageboards"></reference>
		<reference entity="MBMessage" package-path="com.liferay.portlet.messageboards"></reference>
		<reference entity="MBThread" package-path="com.liferay.portlet.messageboards"></reference>
		<reference entity="RatingsEntry" package-path="com.liferay.portlet.ratings"></reference>
		<reference entity="RatingsStats" package-path="com.liferay.portlet.ratings"></reference>
		<reference entity="SocialActivity" package-path="com.liferay.portlet.social"></reference>
	</entity>
	<entity local-service="true" name="CalendarNotificationTemplate" remote-service="true" uuid="true">
		<!-- PK fields -->
		<column name="calendarNotificationTemplateId" primary="true" type="long"></column>
		<!-- Group instance -->
		<column name="groupId" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<!-- Other fields -->
		<column name="calendarId" type="long"></column>
		<column name="notificationType" type="String"></column>
		<column name="notificationTypeSettings" type="String"></column>
		<column name="notificationTemplateType" type="String"></column>
		<column name="subject" type="String"></column>
		<column name="body" type="String"></column>
		<!-- Finder methods -->
		<finder name="CalendarId" return-type="Collection">
			<finder-column name="calendarId"></finder-column>
		</finder>
		<finder name="C_NT_NTT" return-type="CalendarNotificationTemplate">
			<finder-column name="calendarId"></finder-column>
			<finder-column name="notificationType"></finder-column>
			<finder-column name="notificationTemplateType"></finder-column>
		</finder>
	</entity>
	<entity local-service="true" name="CalendarResource" remote-service="true" uuid="true">
		<!-- PK fields -->
		<column name="calendarResourceId" primary="true" type="long"></column>
		<!-- Group instance -->
		<column name="groupId" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<!-- Permission fields -->
		<column name="resourceBlockId" type="long"></column>
		<!-- Other fields -->
		<column name="classNameId" type="long"></column>
		<column name="classPK" type="long"></column>
		<column name="classUuid" type="String"></column>
		<column name="code" type="String"></column>
		<column localized="true" name="name" type="String"></column>
		<column localized="true" name="description" type="String"></column>
		<column name="active" type="boolean"></column>
		<!-- Order -->
		<order by="asc">
			<order-column case-sensitive="false" name="code"></order-column>
		</order>
		<!-- Finder methods -->
		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId"></finder-column>
		</finder>
		<finder name="Active" return-type="Collection">
			<finder-column name="active"></finder-column>
		</finder>
		<finder name="G_C" return-type="Collection">
			<finder-column arrayable-operator="OR" name="groupId"></finder-column>
			<finder-column name="code"></finder-column>
		</finder>
		<finder name="G_A" return-type="Collection">
			<finder-column name="groupId"></finder-column>
			<finder-column name="active"></finder-column>
		</finder>
		<finder name="C_C" return-type="CalendarResource" unique="true">
			<finder-column name="classNameId"></finder-column>
			<finder-column name="classPK"></finder-column>
		</finder>
		<finder name="C_C_A" return-type="Collection">
			<finder-column name="companyId"></finder-column>
			<finder-column comparator="LIKE" name="code"></finder-column>
			<finder-column name="active"></finder-column>
		</finder>
		<!-- References -->
		<reference entity="ClassName" package-path="com.liferay.portal"></reference>
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset"></reference>
	</entity>
	<exceptions>
		<exception>CalendarBookingDuration</exception>
		<exception>CalendarBookingEndTime</exception>
		<exception>CalendarBookingRecurrence</exception>
		<exception>CalendarBookingStartTime</exception>
		<exception>CalendarBookingTitle</exception>
		<exception>CalendarName</exception>
		<exception>CalendarResourceCode</exception>
		<exception>CalendarResourceName</exception>
		<exception>DuplicateCalendarResource</exception>
		<exception>RequiredCalendar</exception>
	</exceptions>
</service-builder>