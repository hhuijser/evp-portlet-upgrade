<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder auto-namespace-tables="false" package-path="com.liferay.sync">
	<namespace>Sync</namespace>
	<entity local-service="true" name="SyncDevice" remote-service="true" uuid="true">
		<!-- PK fields -->
		<column name="syncDeviceId" primary="true" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<!-- Other fields -->
		<column name="type" type="String"></column>
		<column name="buildNumber" type="long"></column>
		<column name="featureSet" type="int"></column>
		<column name="hostname" type="String"></column>
		<column name="status" type="int"></column>
		<!-- Finder methods -->
		<finder name="UserId" return-type="Collection">
			<finder-column name="userId"></finder-column>
		</finder>
		<finder name="C_U" return-type="Collection">
			<finder-column name="companyId"></finder-column>
			<finder-column case-sensitive="false" comparator="LIKE" name="userName"></finder-column>
		</finder>
	</entity>
	<entity local-service="true" name="SyncDLFileVersionDiff" remote-service="false">
		<!-- PK fields -->
		<column name="syncDLFileVersionDiffId" primary="true" type="long"></column>
		<!-- Other fields -->
		<column name="fileEntryId" type="long"></column>
		<column name="sourceFileVersionId" type="long"></column>
		<column name="targetFileVersionId" type="long"></column>
		<column name="dataFileEntryId" type="long"></column>
		<column name="size" type="long"></column>
		<column name="expirationDate" type="Date"></column>
		<!-- Finder methods -->
		<finder name="FileEntryId" return-type="Collection">
			<finder-column name="fileEntryId"></finder-column>
		</finder>
		<finder name="ExpirationDate" return-type="Collection">
			<finder-column comparator="&lt;" name="expirationDate"></finder-column>
		</finder>
		<finder name="F_S_T" return-type="SyncDLFileVersionDiff" unique="true">
			<finder-column name="fileEntryId"></finder-column>
			<finder-column name="sourceFileVersionId"></finder-column>
			<finder-column name="targetFileVersionId"></finder-column>
		</finder>
		<!-- References -->
		<reference entity="Company" package-path="com.liferay.portal"></reference>
		<reference entity="DLApp" package-path="com.liferay.portlet.documentlibrary"></reference>
	</entity>
	<entity local-service="true" name="SyncDLObject" remote-service="true">
		<!-- PK fields -->
		<column name="syncDLObjectId" primary="true" type="long"></column>
		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createTime" type="long"></column>
		<column name="modifiedTime" type="long"></column>
		<!-- Other fields -->
		<column name="repositoryId" type="long"></column>
		<column name="parentFolderId" type="long"></column>
		<column json-enabled="false" name="treePath" type="String"></column>
		<column name="name" type="String"></column>
		<column name="extension" type="String"></column>
		<column name="mimeType" type="String"></column>
		<column name="description" type="String"></column>
		<column name="changeLog" type="String"></column>
		<column name="extraSettings" type="String"></column>
		<column name="version" type="String"></column>
		<column name="versionId" type="long"></column>
		<column name="size" type="long"></column>
		<column name="checksum" type="String"></column>
		<column name="event" type="String"></column>
		<column name="lanTokenKey" type="String"></column>
		<column json-enabled="false" name="lastPermissionChangeDate" type="Date"></column>
		<column name="lockExpirationDate" type="Date"></column>
		<column name="lockUserId" type="long"></column>
		<column name="lockUserName" type="String"></column>
		<column name="type" type="String"></column>
		<column name="typePK" type="long"></column>
		<column name="typeUuid" type="String"></column>
		<!-- Order -->
		<order by="asc">
			<order-column name="modifiedTime"></order-column>
			<order-column name="repositoryId"></order-column>
		</order>
		<!-- Finder methods -->
		<finder name="M_R" return-type="Collection">
			<finder-column comparator="&gt;" name="modifiedTime"></finder-column>
			<finder-column name="repositoryId"></finder-column>
		</finder>
		<finder name="R_P" return-type="Collection">
			<finder-column name="repositoryId"></finder-column>
			<finder-column name="parentFolderId"></finder-column>
		</finder>
		<finder name="R_NotE" return-type="Collection">
			<finder-column name="repositoryId"></finder-column>
			<finder-column comparator="!=" name="event"></finder-column>
		</finder>
		<finder name="R_T" return-type="Collection">
			<finder-column name="repositoryId"></finder-column>
			<finder-column name="type"></finder-column>
		</finder>
		<finder name="V_T" return-type="Collection">
			<finder-column name="version"></finder-column>
			<finder-column name="type"></finder-column>
		</finder>
		<finder name="T_T" return-type="SyncDLObject" unique="true">
			<finder-column name="type"></finder-column>
			<finder-column name="typePK"></finder-column>
		</finder>
		<finder name="M_R_NotE" return-type="Collection">
			<finder-column comparator="&gt;" name="modifiedTime"></finder-column>
			<finder-column name="repositoryId"></finder-column>
			<finder-column arrayable-operator="AND" comparator="!=" name="event"></finder-column>
		</finder>
		<finder name="R_P_T" return-type="Collection">
			<finder-column name="repositoryId"></finder-column>
			<finder-column name="parentFolderId"></finder-column>
			<finder-column arrayable-operator="OR" name="type"></finder-column>
		</finder>
		<!-- References -->
		<reference entity="Group" package-path="com.liferay.portal"></reference>
		<reference entity="Organization" package-path="com.liferay.portal"></reference>
		<reference entity="Repository" package-path="com.liferay.portal"></reference>
		<reference entity="DLApp" package-path="com.liferay.portlet.documentlibrary"></reference>
		<reference entity="DLFileEntry" package-path="com.liferay.portlet.documentlibrary"></reference>
		<reference entity="DLFileVersion" package-path="com.liferay.portlet.documentlibrary"></reference>
		<reference entity="DLFolder" package-path="com.liferay.portlet.documentlibrary"></reference>
		<!-- Transactions -->
		<tx-required>getFileDeltaAsStream</tx-required>
	</entity>
	<entity local-service="true" name="SyncPreferences" remote-service="false" tx-manager="false"></entity>
	<exceptions>
		<exception>SyncDeviceActive</exception>
		<exception>SyncDeviceWipe</exception>
		<exception>SyncDLObjectChecksum</exception>
	</exceptions>
</service-builder>